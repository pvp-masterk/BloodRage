{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/:username([a-zA-Z0-9_-]+)",
      "destination": "/account.html?username=:username"
    }
  ],
  "redirects": [
    {
      "source": "/dashboard",
      "destination": "/dashboard.html",
      "permanent": true
    },
    {
      "source": "/profile",
      "destination": "/profile.html",
      "permanent": true
    }
  ]
}
