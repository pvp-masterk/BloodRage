{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "^/((?!dashboard|profile|_next|static|api|favicon\\.ico|.*\\..*).+)$",
      "dest": "/profile?username=$1",
      "status": 200
    },
    {
      "src": "^/$",
      "dest": "/dashboard",
      "status": 200
    },
    {
      "src": "/dashboard",
      "status": 301
    },
    {
      "src": "/profile",
      "status": 301
    }
  ]
}
