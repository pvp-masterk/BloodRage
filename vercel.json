{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/dashboard"
    },
    {
      "source": "/([^/]+)",
      "destination": "/profile?username=:match"
    }
  ],
  "redirects": [
    {
      "source": "/dashboard.html",
      "destination": "/dashboard",
      "permanent": true
    },
    {
      "source": "/profile.html",
      "destination": "/profile",
      "permanent": true
    }
  ]
}
