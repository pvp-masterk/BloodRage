{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/:username",
      "destination": "/profile.html?username=:username",
      "has": [
        {
          "type": "query",
          "key": "username",
          "value": "(?<username>.*)"
        }
      ]
    },
    {
      "source": "/",
      "destination": "/dashboard.html"
    }
  ],
  "redirects": [
    {
      "source": "/dashboard",
      "destination": "/dashboard.html",
      "permanent": true
    },
    {
      "source": "/profile",
      "destination": "/profile.html",
      "permanent": true
    }
  ]
}
