{
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/",
      "destination": "/dashboard",
      "statusCode": 302
    }
  ],
  "rewrites": [
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/edit",
      "destination": "/edit.html"
    },
    {
      "source": "/profile",
      "destination": "/profile.html"
    },
    {
      "source": "/:username",
      "destination": "/profile.html"
    }
  ]
}
