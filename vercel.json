{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/dashboard.html"
    },
    {
      "source": "/([a-zA-Z0-9_-]+)",
      "destination": "/profile.html?username=:match"
    }
  ],
  "redirects": [
    {
      "source": "/dashboard",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/profile",
      "destination": "/profile.html",
      "permanent": true
    }
  ]
}
