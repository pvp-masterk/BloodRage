{
  "routes": [
    
    {
      "src": "/(.*)\\.html",
      "dest": "/$1",
      "status": 301
    },
    
    
    {
      "src": "/profile",
      "status": 404
    },
    
    
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    
    
    {
      "src": "/@([a-z0-9_]+)",
      "dest": "/profile.html?user=$1",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    
    
    {
      "src": "/",
      "dest": "/index.html"
    },
    
    
    {
      "src": "/([a-z0-9-]+)",
      "dest": "/$1.html",
      "check": true
    }
  ]
}
