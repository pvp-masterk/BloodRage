{
  "routes": [
    {
      "src": "/(.*)\\.html",
      "dest": "/$1",
      "status": 301
    },
    {
      "src": "/profile",
      "status": 404
    },
    {
      "src": "/:username",
      "dest": "/profile.html",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/$1.html",
      "check": true
    }
  ]
}
