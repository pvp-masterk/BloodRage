{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "^/(?!(dashboard|profile|_next|static|api|favicon\\.ico|.*\\..*))[^/]+$",
      "dest": "/profile.html?username=$1",
      "status": 200
    },
    {
      "src": "^/$",
      "dest": "/dashboard.html",
      "status": 200
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard.html",
      "status": 301
    },
    {
      "src": "/profile",
      "dest": "/profile.html",
      "status": 301
    }
  ]
}
