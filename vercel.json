{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/:username((?!dashboard|profile|_next|static|api|favicon\\.ico|.*\\..*).+)",
      "destination": "/profile?username=:username"
    }
  ],
  "redirects": [
    {
      "source": "/dashboard",
      "destination": "/dashboard",
      "permanent": true
    },
    {
      "source": "/profile",
      "destination": "/profile",
      "permanent": true
    }
  ]
}
