{
  "routes": [
    {
      "src": "/profile\\.html",
      "status": 404
    },
    {
      "src": "/profile",
      "status": 404
    },
    {
      "src": "/([^/]+)",  // Matches any single path segment (username)
      "dest": "/profile.html",
      "headers": {
        "x-username": ":1"  // Pass the username as a header
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1.html",
      "check": true
    }
  ]
}
